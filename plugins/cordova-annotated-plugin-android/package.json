{
  "name": "cordova-annotated-plugin-android",
  "version": "1.0.4",
  "description": "Enhanced CordovaPlugin class",
  "cordova": {
    "id": "cordova-annotated-plugin-android",
    "platforms": [
      "ios",
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appfeel/cordova-annotated-plugin-android.git"
  },
  "author": "Miquel Martin <info@appfeel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appfeel/cordova-annotated-plugin-android/issues"
  },
  "homepage": "https://github.com/appfeel/cordova-annotated-plugin-android#readme",
  "keywords": [
    "cordova",
    "ecosystem:cordova",
    "cordova-android"
  ],
  "scripts": {
    "release": "release-it",
    "version": "cross-var replace-in-file '/<plugin(.*)(\\n?)(.*)version=\"(.*)\"/ig' '<plugin$1$2$3version=\"$npm_package_version\"' ./plugin.xml --isRegex && git add plugin.xml"
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "release-it": "^8.1.0",
    "replace-in-file": "^3.4.2"
  }
}
